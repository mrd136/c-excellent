<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_hms_acs_vaccination" model="ir.sequence">
            <field name="name">ACS Vaccination</field>
            <field name="code">acs.vaccination</field>
            <field name="prefix">VC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="base.main_company" model="res.company" >
            <field name="vaccination_usage_location" ref="stock.stock_location_customers"/>
            <field name="vaccination_stock_location" ref="stock.stock_location_stock"/>
        </record>

    </data>
</odoo>
