<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_surgery" model="ir.sequence">
            <field name="name">ACS Surgery</field>
            <field name="code">hms.surgery</field>
            <field name="prefix">SUG</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="base.main_company" model="res.company" >
            <field name="surgery_usage_location" ref="stock.stock_location_customers"/>
            <field name="surgery_stock_location" ref="stock.stock_location_stock"/>
        </record>

    </data>
</odoo>
