<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_hms_referral" model="ir.sequence">
            <field name="name">ACS Referral</field>
            <field name="code">hms.referral</field>
            <field name="prefix">RE/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hms_multi_referral" model="ir.sequence">
            <field name="name">Multi Referral</field>
            <field name="code">hms.multi.referral</field>
            <field name="prefix">M-RE/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data>
        <record id="ir_cron_auto_close_referral_request" model="ir.cron">
            <field name="name">Auto Close for referral</field>
            <field name="model_id" ref="model_hms_referral"/>
            <field name="state">code</field>
            <field name="code">model.auto_close_referral()</field>
<!--            <field name="user_id" ref="base.user_root" />-->
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>
