<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="default_barcode_configuration" model="barcode.configuration">
            <field name="label_width" eval="100" />
            <field name="label_height" eval="100" />
            <field name="margin_top" eval="1" />
            <field name="margin_bottom" eval="1" />
            <field name="margin_left" eval="1" />
            <field name="margin_right" eval="1" />
            <field name="dpi" eval="90" />
            <field name="header_spacing" eval="1" />
            <field name="display_width" eval="230" />
            <field name="display_height" eval="50" />
            <field name="currency_position">before</field>
            <field name="humanreadable" eval="True" />
            <field name="product_name" eval="True" />
            <field name="product_code" eval="True" />
            <field name="description" eval="True" />
            <field name="quantity" eval="True" />
            <field name="barcode" eval="True" />
            <field name="lot" eval="True"/>
            <field name="barcode_field">barcode</field>
        </record>
    </data>
</odoo>
